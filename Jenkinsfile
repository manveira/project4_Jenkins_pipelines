pipeline {
    agent any

    stages {
      
        stage('Clone') {
          steps {
            echo "cloning"
            echo "repo 4"        
          }

        }

        stage('Build') {
          steps {
            echo "Building"
          }      
        }

        stage('Test') {
          steps {
            echo "Testing"
          }      
        }

        stage('Deploy') {
          steps {
            echo "Applying"
          }      
        }            
    } 
}